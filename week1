#include <stdio.h>

//function main begins program execution
int main(void)
{
	int integer1;
	int integer2;
	int sum;

	printf("enter first integer\n");
	scanf_s("%d", &integer1);

	printf("enter second integer\n");
	scanf_s("%d", &integer2);

	sum = integer1 + integer2;

	printf("sum is %d\n", sum);

}//end function main
